<% @webpage.title = "New PhotoSet" -%>
<% content_for 'head' do -%>
  <%= javascript_include_tag :defaults %>
<% end -%>

<%= error_messages_for :photo_set %>

<% form_for(:photo_set, :url => formatted_photo_sets_url(:format => :html)) do |f| %>
  <%= render(:partial => 'form', :locals => { :form => f }) %>
  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', formatted_photo_sets_url(:format => :html) %>
